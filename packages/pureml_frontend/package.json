    "@radix-ui/react-dropdown-menu": "^2.0.1",
    "@radix-ui/react-select": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.0.2",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "@remix-run/server-runtime": "^1.10.0",
    "@remix-run/vercel": "^1.11.0",
    "@tanstack/react-table": "^8.7.9",
    "@types/react-calendar-heatmap": "^1.6.3",
    "@vercel/node": "^1.8.3 || ^2.4.0",
    "airtable": "^0.11.6",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "csstype": "^3.0.10",
    "dagre": "^0.8.5",
    "daisyui": "^2.50.0",
    "isbot": "^3.6.5",
    "lucide-react": "^0.105.0",
    "marked": "^4.2.12",
    "node-fetch": "2.6.7",
    "postcss": ">=8.3.3 <9.0.0",
    "posthog-js": "^1.51.5",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-calendar-heatmap": "^1.9.0",
    "react-code-blocks": "0.0.9-0",
    "react-dom": "^18.2.0",
    "react-is": ">=16.8.0",
    "react-quilljs": "^1.3.3",
    "react-simply-carousel": "^8.5.2",
    "react-simply-tabs": "^1.0.4",
    "react-toastify": "^9.1.1",
    "reactflow": "^11.4.2",
    "remix": "^1.11.1",
    "remix-utils": "^6.0.0",
    "tailwind-variants": "^0.0.30",
    "tailwindcss-radix": "^2.6.2",
    "zod": "^3.20.2"
