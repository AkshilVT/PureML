  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@tailwindcss/forms": "^0.5.3",
    "@types/dagre": "^0.7.48",
    "@types/marked": "^4.0.8",
    "@types/react": "17.0.0",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.2",
    "tailwindcss": "^3.3.1",
